{
  "name": "@morlay/redux-actions",
  "version": "0.0.0",
  "main": "lib/index.js",
  "dependencies": {
    "lodash": "4.x",
    "redux-actions": "0.10.x"
  },
  "devDependencies": {
    "@morlay/babel-preset": "1.0.x",
    "@morlay/eslint-config": "1.0.x",
    "@morlay/tests": "1.0.x",
    "babel-preset-es2015": "6.9.x",
    "babel-plugin-lodash": "3.2.x"
  },
  "config": {
    "MOCHA_OPTS": "-R spec --compilers js:babel-core/register src/__tests__/*.spec.js"
  },
  "scripts": {
    "build": "babel src/ --ignore __*__ --out-dir lib/",
    "lint": "eslint ./ --fix",
    "mocha": "mocha $npm_package_config_MOCHA_OPTS",
    "test": "npm run lint && npm run mocha",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morlay/redux-actions.git"
  },
  "author": "Morlay Null <morlay.null@gmail.com>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/morlay/redux-actions/issues"
  },
  "homepage": "https://github.com/morlay/redux-actions#readme"
}
